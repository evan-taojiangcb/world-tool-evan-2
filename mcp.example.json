{
  "mcp_servers": {
    "stitch": {
      "url": "https://stitch.googleapis.com/mcp",
      "http_headers": {
        "X-Goog-Api-Key": "REPLACE_WITH_YOUR_STITCH_API_KEY"
      }
    },
    "aws-mcp": {
      "command": "uvx",
      "timeout": 100000,
      "transport": "stdio",
      "args": [
        "mcp-proxy-for-aws@latest",
        "https://aws-mcp.us-east-1.api.aws/mcp",
        "--metadata",
        "AWS_REGION=ap-northeast-1"
      ]
    }
  }
}
